<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.icu.yankiinsel.icu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".Activities.ProfileActivity"/>
        <activity android:label="@string/app_name" android:name=".Activities.PreferencesActivity" android:windowSoftInputMode="adjustResize" />
        <activity android:label="@string/action_matches" android:name=".Activities.MatchesActivity" />
        <activity android:label="" android:name=".Activities.MessageListActivity" />

    </application>

</manifest>